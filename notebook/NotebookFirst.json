				"source": [
					"# Import necessary libraries\r\n",
					"from pyspark.sql import SparkSession\r\n",
					"\r\n",
					"# Set up Spark session with Azure Data Lake Storage configurations\r\n",
					"spark = SparkSession.builder \\\r\n",
					"    .appName(\"Read CSV\") \\\r\n",
					"    .config(\"fs.azure.account.auth.type.<your_account_name>.dfs.core.windows.net\", \"OAuth\") \\\r\n",
					"    .config(\"fs.azure.account.oauth.provider.type.<your_account_name>.dfs.core.windows.net\", \"org.apache.hadoop.fs.azurebfs.oauth2.ClientCredsTokenProvider\") \\\r\n",
					"    .config(\"fs.azure.account.oauth2.client.id.<your_account_name>.dfs.core.windows.net\", \"<your_client_id>\") \\\r\n",
					"    .config(\"fs.azure.account.oauth2.client.secret.<your_account_name>.dfs.core.windows.net\", \"<your_client_secret>\") \\\r\n",
					"    .config(\"fs.azure.account.oauth2.client.endpoint.<your_account_name>.dfs.core.windows.net\", \"https://login.microsoftonline.com/<your_tenant_id>/oauth2/token\") \\\r\n",
					"    .getOrCreate()\r\n",
					"\r\n",
					"# Read the CSV file from Azure Data Lake Storage\r\n",
					"csv_file_path = \"abfss://newsynapsefilesys@newsynapseadlsaccount.dfs.core.windows.net/raw-data/Data/sdData.csv\"\r\n",
					"df = spark.read.csv(csv_file_path, header=True, inferSchema=True)\r\n",
					""
				],
